                logger.info(f"üî• DERIV ARROW + CONDITIONS AGRESSIVES R√âUNIES pour {symbol}")
                if self.aggressive_strategy.activate_strategy(symbol, aggressive_signal_data):
                    logger.info(f"üöÄ Strat√©gie agressive activ√©e via DERIV ARROW pour {symbol}")
                    return True
                else:
                    logger.error(f"‚ùå Erreur activation strat√©gie agressive via DERIV ARROW pour {symbol}")
            
            # V√©rifier si une strat√©gie agressive est d√©j√† active
            if symbol in self.aggressive_strategy.active_strategies:
                logger.info(f"üîÑ Strat√©gie agressive d√©j√† active pour {symbol}, pas de trade DERIV ARROW normal")
                return False
            
            # Obtenir les informations du symbole
            symbol_info = mt5.symbol_info(symbol)
            if not symbol_info:
                logger.error(f"Impossible d'obtenir les infos pour {symbol}")
                return False
                
            tick = mt5.symbol_info_tick(symbol)
            if not tick:
                logger.error(f"Impossible d'obtenir le tick pour {symbol}")
                return False
                
            # D√©terminer le prix et le type d'ordre
            if signal == 'BUY':
                price = tick.ask
                order_type = mt5.ORDER_TYPE_BUY
                sl = tick.ask - (symbol_info.point * 50)  # SL tr√®s serr√©: 50 points
                tp = tick.ask + (symbol_info.point * 150)  # TP rapide: 150 points
            else:  # SELL
                price = tick.bid
                order_type = mt5.ORDER_TYPE_SELL
                sl = tick.bid + (symbol_info.point * 50)  # SL tr√®s serr√©: 50 points
                tp = tick.bid - (symbol_info.point * 150)  # TP rapide: 150 points
            
            # VALIDATION CRUCIALE: V√©rifier et corriger les distances SL/TP minimales
            # √âviter les erreurs "Invalid stops" sur USDJPY et autres symboles Forex
            stop_level = getattr(symbol_info, 'trade_stops_level', 0) or getattr(symbol_info, 'stops_level', 0) or 0
            min_distance = stop_level * symbol_info.point if stop_level > 0 else symbol_info.point * 5
            
            sl_distance = abs(price - sl)
            tp_distance = abs(tp - price)
            
            # Corriger SL si trop proche
            if sl_distance < min_distance:
                if signal == 'BUY':
                    sl = price - min_distance
                else:  # SELL
                    sl = price + min_distance
                logger.info(f"üîß SL corrig√© pour distance minimale: {symbol} | Nouveau SL: {sl}")
            
            # Corriger TP si trop proche
            if tp_distance < min_distance:
                if signal == 'BUY':
                    tp = price + min_distance
                else:  # SELL
                    tp = price - min_distance
                logger.info(f"üîß TP corrig√© pour distance minimale: {symbol} | Nouveau TP: {tp}")
            
            # Validation finale: s'assurer que SL/TP sont valides
            if signal == 'BUY':
                if sl >= price or tp <= price:
                    logger.error(f"‚ùå SL/TP invalides pour BUY sur {symbol} - SL: {sl} >= Prix: {price} ou TP: {tp} <= Prix: {price}")
                    return False
            else:  # SELL
                if sl <= price or tp >= price:
                    logger.error(f"‚ùå SL/TP invalides pour SELL sur {symbol} - SL: {sl} <= Prix: {price} ou TP: {tp} >= Prix: {price}")
                    return False
                
            # Taille de position selon la cat√©gorie
            lot_size = self.symbol_detector.get_position_size(symbol)
            
            # Normaliser la taille
            lot_size = max(symbol_info.volume_min, min(lot_size, symbol_info.volume_max))
            lot_size = round(lot_size / symbol_info.volume_step) * symbol_info.volume_step
            
            # Pr√©parer la requ√™te
            request = {
                "action": mt5.TRADE_ACTION_DEAL,
                "symbol": symbol,
                "volume": lot_size,
                "type": order_type,
                "price": price,
                "sl": sl,
                "tp": tp,
                "deviation": 20,
                "magic": 888888,
                "comment": f"DERIV_ARROW_{signal}",
                "type_time": mt5.ORDER_TIME_GTC,
                "type_filling": self.get_symbol_filling_mode(symbol)
            }
            
            # Envoyer l'ordre
            result = mt5.order_send(request)
            
            if result.retcode == mt5.TRADE_RETCODE_DONE:
                ticket = result.order
                logger.info(f"‚ö° DERIV ARROW Trade ouvert: {signal} | {symbol} | Lot: {lot_size} | Ticket: {ticket} | SL: {sl} | TP: {tp}")
                
                # Envoyer notification
                trade_logger_instance.log_trade_success(symbol, signal, ticket)
                
                # Ajouter aux positions
                self.positions[ticket] = {
                    "symbol": symbol,
                    "type": signal,
                    "volume": lot_size,
                    "price": price,
                    "sl": sl,
                    "tp": tp,
                    "ticket": ticket,
                    "open_time": datetime.now(),
                    "max_profit": 0.0
                }
                
                # Envoyer le feedback
                self.send_trade_feedback(symbol, signal_data, "DERIV_ARROW_EXECUTED")
                
                return True
            else:
                logger.error(f"‚ùå Erreur ouverture trade DERIV ARROW {symbol}: {result.retcode} - {result.comment}")
                trade_logger_instance.log_trade_error(symbol, signal, result.retcode, result.comment, request["type_filling"])
                return False
                
        except Exception as e:
            logger.error(f"Erreur ex√©cution trade DERIV ARROW {symbol}: {e}")
            return False

    def monitor_positions_protection(self):
        """
        Suivi des positions pour prot√©ger les gains et limiter les pertes
        - Ferme la position si elle perd plus de 50% du gain maximum d√©j√† acquis
        - Ferme la position si la perte d√©passe 6 dollars
        """
        try:
            positions = mt5.positions_get()
            if not positions:
                return
                
            for position in positions:
                ticket = position.ticket
                symbol = position.symbol
                current_profit = position.profit
                open_price = position.price_open  # CORRIG√â: open_price ‚Üí price_open
                current_price = position.price_current
                position_type = position.type
                
                # R√©cup√©rer l'historique des profits pour cette position
                max_profit = self.get_position_max_profit(ticket)
                
                if max_profit is None:
                    max_profit = current_profit
                    self.save_position_max_profit(ticket, max_profit)
                
                # Protection 1: Limiter la perte √† 6 dollars (SAUF pour Boom/Crash)
                # Pour Boom/Crash: ne jamais fermer automatiquement, laisser aller au bout
                if not boom_crash_manager.should_close_automatically(symbol, current_profit):
                    if current_profit <= -6.0:
                        logger.warning(f"üõë PROTECTION PERTE 6$ - Fermeture position {ticket} ({symbol}) | Perte: ${current_profit:.2f}")
                        self.close_position_with_protection(ticket, "Loss_6_Dollars")
                        continue
                
                # Protection 2: Prot√©ger 50% des gains max acquis
                if max_profit > 0:  # Uniquement si la position a √©t√© en profit
                    max_allowed_loss = max_profit * 0.5  # 50% du gain max
                    current_loss_from_max = max_profit - current_profit
                    
                    if current_loss_from_max >= max_allowed_loss:
                        logger.warning(f"üõ°Ô∏è PROTECTION GAINS - Fermeture position {ticket} ({symbol}) | Gain max: ${max_profit:.2f} | Actuel: ${current_profit:.2f} | Perte depuis max: ${current_loss_from_max:.2f}")
                        self.close_position_with_protection(ticket, "Protect_50_Percent_Gains")
                        continue
                
                # Mise √† jour du profit maximum si n√©cessaire
                if current_profit > max_profit:
                    self.save_position_max_profit(ticket, current_profit)
                    logger.info(f"üìà Nouveau gain max position {ticket}: ${current_profit:.2f}")
                
        except Exception as e:
            logger.error(f"Erreur monitoring positions protection: {e}")
    
    def get_position_max_profit(self, ticket):
        """R√©cup√®re le profit maximum historique pour une position"""
        try:
            # Utiliser un fichier JSON pour stocker les profits max par position
            import json
            from pathlib import Path
            
            profits_file = Path("position_max_profits.json")
            if profits_file.exists():
                with open(profits_file, 'r') as f:
                    profits_data = json.load(f)
                    return profits_data.get(str(ticket), None)
            return None
        except Exception as e:
            logger.error(f"Erreur lecture profit max position {ticket}: {e}")
            return None
    
    def save_position_max_profit(self, ticket, profit):
        """Sauvegarde le profit maximum pour une position"""
        try:
            import json
            from pathlib import Path
            
            profits_file = Path("position_max_profits.json")
            profits_data = {}
            
            # Charger les donn√©es existantes
            if profits_file.exists():
                with open(profits_file, 'r') as f:
                    profits_data = json.load(f)
            
            # Mettre √† jour le profit maximum
            profits_data[str(ticket)] = profit
            
            # Sauvegarder
            with open(profits_file, 'w') as f:
                json.dump(profits_data, f, indent=2)
                
        except Exception as e:
            logger.error(f"Erreur sauvegarde profit max position {ticket}: {e}")
    
    def close_position_with_protection(self, ticket, reason):
        """Ferme une position avec logging de protection"""
        try:
            position = mt5.positions_get(ticket=ticket)
            if not position:
                return False
                
            position = position[0]
            symbol = position.symbol
            profit = position.profit
            
            # Fermer la position
            result = mt5.Close(symbol, ticket)
            
            if result.retcode == mt5.TRADE_RETCODE_DONE:
                logger.info(f"‚úÖ Position ferm√©e avec protection | Ticket: {ticket} | Symbol: {symbol} | Profit: ${profit:.2f} | Raison: {reason}")
                
                # Nettoyer l'enregistrement du profit maximum
                self.cleanup_position_max_profit(ticket)
                
                # Logger dans le fichier de trades
                trade_logger_instance.log_position_update(symbol, ticket, f"CLOSED_PROTECTION_{reason}", profit=profit)
                
                return True
            else:
                logger.error(f"‚ùå Erreur fermeture position protection {ticket}: {result.comment}")
                return False
                
        except Exception as e:
            logger.error(f"Erreur fermeture position protection {ticket}: {e}")
            return False
    
    def cleanup_position_max_profit(self, ticket):
        """Nettoie l'enregistrement du profit maximum pour une position ferm√©e"""
        try:
            import json
            from pathlib import Path
            
            profits_file = Path("position_max_profits.json")
            if profits_file.exists():
                with open(profits_file, 'r') as f:
                    profits_data = json.load(f)
                
                # Supprimer l'entr√©e pour cette position
                if str(ticket) in profits_data:
                    del profits_data[str(ticket)]
                    
                    # Sauvegarder les donn√©es mises √† jour
                    with open(profits_file, 'w') as f:
                        json.dump(profits_data, f, indent=2)
                        
        except Exception as e:
            logger.error(f"Erreur nettoyage profit max position {ticket}: {e}")

