                self.symbol_blacklist = {}
            
            self.symbol_blacklist[symbol] = {
                'reason': reason,
                'timestamp': time.time(),
                'duration': 300  # 5 minutes de blocage
            }
            
            logger.warning(f"üö´ SYMBOLE BLOQU√â: {symbol} | Raison: {reason} | Dur√©e: 5 minutes")
            
        except Exception as e:
